{
  "expo": {
    "name": "LYST",
    "slug": "lyst-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#E8F2FF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lyst.app"
    },
    "android": {
      "package": "com.lyst.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E8F2FF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static",
      "themeColor": "#1E3A6E"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ]
  }
}
