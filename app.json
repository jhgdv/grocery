{
  "expo": {
    "name": "LYST",
    "slug": "lyst-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#151515"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lyst.app"
    },
    "android": {
      "package": "com.lyst.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#151515"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static",
      "themeColor": "#151515"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos so you can attach them to list items.",
          "cameraPermission": "The app needs access to your camera so you can take photos to attach to list items."
        }
      ]
    ]
  }
}